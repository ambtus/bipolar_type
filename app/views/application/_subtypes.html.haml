%table.center.half
  -subtypes.in_groups_of(2).each do |row|
    %tr
      -row.each do |subtype|
        %td
          =link_to subtype.name_with_mbti, type_path(subtype.path)
%h2 Balanced
%table.center.half
  -subtypes.in_groups_of(2).each do |row|
    %tr
      -row.each do |subtype|
        %td
          =render "balanced", :subtype => subtype
%h2 Unbalanced
%table.center.half
  -subtypes.in_groups_of(2).each do |row|
    %tr
      -row.each do |subtype|
        %td
          =render "unbalanced", :subtype => subtype
