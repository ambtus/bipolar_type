- content_for(:title, Behavior.title)


%table
  %caption
    %h2 Sixteen Behaviors
  -Behavior.sorted.in_groups_of(2).each_with_index do |row, index|
    %tr
      -row.each do |behavior|
        %td.left.bordered=render 'cookie', thing: behavior
      -if index.odd?
        %tr
          %th{colspan:2} &nbsp;
