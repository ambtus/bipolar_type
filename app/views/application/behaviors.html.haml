- content_for(:title, Behavior.title)

%h2 The Eight Behaviors

%table
  %tr
    %th
    -Action.each do |thing|
      %th=formatted thing
  -Behavior.all.in_groups_of(2).each do |row|
    %tr
      %th=formatted row.first.focus
      -row.each do |behavior|
        %td.left.bordered=render 'cookie', thing: behavior
