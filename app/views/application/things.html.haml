- content_for(:title, Thing.title)

%table
  %tr
    %th
    -Thing::SYMBOL.each do |symbol|
      %th #{symbol} Things

  -Help.each do |help|
    %tr
      %th=help.name
      -Thing.all.each do |thing|
        %td
          =format_from_key thing.symbol + help.symbol
          .italic=format_from_key (thing + help.tendency).symbol
