%table
  %tr
    %th
    -Realm.all.each do |realm|
      -I18n.locale = realm.locale
      %th=link_to realm.name, realm_path(realm.path)
  -%w{domain sense resources consume reserves inactive produce achieve goals neuro}.each do |word|
    %tr
      -I18n.locale = :en
      %th=(t word).titleize
      -Realm.all.each do |realm|
        -I18n.locale = realm.locale
        %td=t word
