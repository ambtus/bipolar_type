- content_for(:title, "The Four Realms")
%table
  %tr
    %th
    -Realm.all.each do |realm|
      %th=link_to realm.name, realm_path(realm.path)
  -%w{power strength energy reserves empty refuel get sense process resources basic necessities tempting extras achieve easy hard goal part}.each do |word|
    %tr
      %th=word
      -Realm.all.each do |realm|
        %td=realm.send(word)
