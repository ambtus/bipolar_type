- content_for(:title, "The Four Realms")
=render "header"
=render "affect"
%table
  %caption.heading The Four Realms
  %tr
    %th.narrow
    -Realm.each do |realm|
      %th=realm.name
  -%w{ energy reserves strengths harvest process resources basic_input painful_input input_pain neuro achieve goals basic_output harder painful_output output_pain question}.each do |word|
    %tr
      %th=word.titleize
      -Realm.each do |realm|
        %td=realm.send(word)
