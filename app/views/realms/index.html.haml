- content_for(:title, "The Four Realms")
%table
  %tr
    %th.narrow
    -Realm.each do |realm|
      %th=link_to realm.name, realm_path(realm.path)
  -%w{energy get_energy intake_pain  intake  output output_pain  use_energy  strength reserves}.each do |word|
    %tr
      %th=word.titleize
      -Realm.each do |realm|
        %td=realm.send(word)
