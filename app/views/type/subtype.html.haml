- content_for(:title, @subtype.name)
=render "subtype", :subtype => @subtype
%h2 Realm
=render "realm", :realm => @subtype.realm
%h2 Attitude
=render "attitude", :attitude => @subtype.attitude
%h2 Mania
=render "mania", :subtype => @subtype
%h2 Depression
=render "depression", :subtype => @subtype
%h2 BipolarTypes
%p.heading=link_to "discover", discover_path("Q2_#{@subtype.path}")
%dl
  -@subtype.quads.each do |quad|
    %dt=link_to quad.name, type_path(quad.path)
    %dd=(quad.subtypes - [@subtype]).map(&:name).join(", ")
