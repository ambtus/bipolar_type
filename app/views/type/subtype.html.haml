- content_for(:title, @subtype.name_with_mbti.html_safe)
%h2 Balanced
=render "balanced", :subtype => @subtype
%h2 Unbalanced
=render "unbalanced", :subtype => @subtype
%h2 Associated BipolarTypes
%dl
  -@subtype.quads.each do |quad|
    %dt=link_to quad.mbti, type_path(quad.path)
    %dd=quad.names.join(", ")
